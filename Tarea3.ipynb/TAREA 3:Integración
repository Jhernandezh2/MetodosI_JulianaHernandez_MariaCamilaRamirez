        "PUNTO 10"
      ],
      "metadata": {
        "id": "_VseVrZIq_MY"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import sympy as sp\n",
        "\n",
        "x, h = sp.symbols('x h')\n",
        "f = sp.Function('f')\n",
        "n = 3\n",
        "derivadaenesima = sp.diff(f(x),x, n+1)\n",
        "e = derivadaenesima.subs(x, sp.Symbol('ξ'))/sp.factorial(n+1)\n",
        "\n",
        "integral = sp.expand(x * (x - h) * (x - 2*h) * (x - 3*h))\n",
        "I = sp.integrate(integral, (x, 0, 3*h))\n",
        "I = sp.simplify(I)* e\n",
        "print(I)\n"
      ],
