        "import numpy as np\n",
        "def funcion(x):\n",
        "   return np.exp(-x**2)"
      ],
      "metadata": {
        "id": "VPGU87_bO0LD"
      },
      "execution_count": 148,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def trapecio(f,a,b,n):\n",
        "  x = np.linspace(a,b,n+1)\n",
        "  y = f(x)\n",
        "  h = (b-a)/n\n",
        "  integral = (h/2)*(np.sum(y[:-1]+y[1:]))\n",
        "  return integral\n",
        ""
      ],
      "metadata": {
        "id": "odsPfnoKOpnh"
